<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.web.WebView?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10"
          stylesheets="@styles.css" fx:id="gridPane">

    <Button onAction="#handleClick" text="Open... " GridPane.rowIndex="0" GridPane.columnIndex="0" />
    <Button text="Button two " GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Button id="Button_three" text="Button three " GridPane.rowIndex="0" GridPane.columnIndex="2"/>
    <Button text="Button four " fx:id="button" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
    <RadioButton text="New Button" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
    <Label text="JavaFx Effects" rotate="90" fx:id="label" GridPane.columnIndex="1" GridPane.rowIndex="2" onMouseEntered="#handleMouseClick" onMouseExited="#handleMouseExit"/>
    <Hyperlink fx:id="link" text="www.javafx.com" GridPane.rowIndex="3" GridPane.columnIndex="2"/>

    </GridPane>